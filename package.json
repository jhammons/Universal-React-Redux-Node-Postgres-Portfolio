{
  "name": "jg",
  "version": "1.3.3",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node server/app.js",
    "nodemon": "nodemon --watch server server/app.js",
    "dev": ". ./secrets.sh && docker-compose up -d",
    "dev:logs": ". ./secrets.sh && docker-compose up",
    "prod": ". ./secrets.sh && NODE_ENV=production docker-compose up -d",
    "prod:build": "npm run prod && npm run build",
    "build": "docker exec -it jasongallagher npm run build:script",
    "build:script": "rm -rf dist && NODE_ENV=production webpack --mode production --config ./webpack.production.config.js",
    "docker:build": ". ./secrets.sh && docker-compose build",
    "docker:stop": "docker-compose stop",
    "docker:down": "docker-compose down",
    "docker:logs": "docker logs -f jasongallagher",
    "docker:attach": "docker exec -it jasongallagher /bin/bash",
    "docker:attach:db": "docker exec -it db bash -c 'psql -U ${POSTGRES_USER} -d ${POSTGRES_DB}'",
    "test": "docker exec -it jasongallagher mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test --require ignore-styles",
    "test:watch": "docker exec -it jasongallagher npm run test -- --watch",
    "seed": "docker exec -it jasongallagher node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "docker exec -it jasongallagher node_modules/.bin/sequelize db:seed:undo:all",
    "stats": "npm run stats:build && npm run stats:display",
    "stats:build": "docker exec -it jasongallagher bash -c 'webpack --config ./webpack.production.config.js --profile --json > stats.json'",
    "stats:display": "docker exec -it jasongallagher webpack-bundle-analyzer stats.json dist -h 0.0.0.0"
  },
